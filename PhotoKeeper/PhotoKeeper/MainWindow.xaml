<Window x:Class="PhotoKeeper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PhotoKeeper"
        mc:Ignorable="d"
        Title="PhotoKeeper" Height="600" Width="1000">
    <Grid  Background="#FFDBFFC6">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="251*"/>
            <ColumnDefinition Width="749*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="65*"/>
            <RowDefinition Height="519*"/>
        </Grid.RowDefinitions>

        <Frame x:Name="MainFrame" Grid.ColumnSpan="2" Grid.RowSpan="2" NavigationUIVisibility="Hidden"></Frame>

        <Frame x:Name="TopFrame" Grid.Column="1" NavigationUIVisibility="Hidden"></Frame>

        <Frame x:Name="LeftFrame" Grid.RowSpan="2" NavigationUIVisibility="Hidden"></Frame>

        <Frame x:Name="ValueFrame" Grid.Row="1" Grid.Column="1" NavigationUIVisibility="Hidden" Margin="0,7,0,0"></Frame>

    </Grid>
</Window>
